package practice;

import java.util.Scanner;

public class Practice {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();

        int oddOne;

        if (a == b) {
            oddOne = c;
        } else if (a == c) {
            oddOne = b;
        } else {
            oddOne = a;
        }

        System.out.println(oddOne);

        sc.close();
    }
}
