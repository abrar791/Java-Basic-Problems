package practice;

import java.util.Scanner;

public class Practice {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int X = sc.nextInt();
        int Y = sc.nextInt();

        int totalHours = X * 4 + Y;
        System.out.println(totalHours);

        sc.close();
    }
}
