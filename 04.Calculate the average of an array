package practice;

public class Practice {
    public static void main(String[] args) {
        int[] numbers = {10, 25, 5, 40, 15};

        int sum = 0;

        for (int i = 0; i < numbers.length; i++) {
            sum += numbers[i];
        }

        double average = (double) sum / numbers.length;

        System.out.println("The average is: " + average);
    }
}
